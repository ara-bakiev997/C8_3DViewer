#test test_calculation_error

  char *test_str = "atan(10)/0"; // 0
  printf("input = %s\n", test_str);
  int error = 0;
  double res = 0;
  error = main_solution(test_str, &res);
  ck_assert_int_eq(error, 1);

  test_str = "cos(pi)/0"; // -1
  printf("input = %s\n", test_str);
  error = 0;
  res = 0;
  error = main_solution(test_str, &res);
  ck_assert_int_eq(error, 1);


